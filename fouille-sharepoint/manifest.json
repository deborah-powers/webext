{
	"manifest_version": 2,
	"name": "fouille sharepoint",
	"version": "1.0",
	"description": "fouiller le sharepoint d'axyus afin de récupérer des infos sur les fichiers",
	"icons": { "48": "icon-128.svg" },
	"background": { "scripts": [ "service-worker-v2.js" ]},
	"permissions": [ "tabs", "activeTab", "scripting", "http://deborah-powers.fr/", "http://localhost:1407/", "file:///" ],
	"content_security_policy": "script-src 'self' 'unsafe-eval'; style-src 'self' 'unsafe-inline'; object-src 'self';",
	"browser_action": {
		"default_icon": "icon-128.svg",
		"default_title": "fouille sharepoint"
	},
	"browser_specific_settings": { "gecko": { "id": "fouilleur@deborah-powers.fr" }}
}