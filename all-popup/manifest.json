{
	"manifest_version": 2,
	"name": "afficher popup",
	"version": "1.2",
	"description": "afficher une popup avec un manifest v2",
	"icons": { "48": "icon-128.png" },
	"permissions": [ "tabs", "activeTab", "scripting" ],
	"browser_action": {
		"default_icon": "icon-128.png",
		"default_title": "wef popup",
		"default_popup": "popup.html"
	},
	"browser_specific_settings": { "gecko": { "id": "popup@deborah-powers.fr" }}
}