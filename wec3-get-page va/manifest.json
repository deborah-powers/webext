{
	"manifest_version": 3,
	"name": "enregistreur de pages",
	"version": "1.0",
	"description": "nettoyer et sauvegarder mes pages web",
	"icons": { "48": "icon-128.png" },
	"permissions": [ "storage", "tabs", "activeTab", "scripting" ],
	"action": {
		"default_icon": { "48": "icon-128.png" },
		"default_title": "afficher la popup",
		"default_popup": "popup.html"
	}
}