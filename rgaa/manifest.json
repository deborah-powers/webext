{
	"manifest_version": 2,
	"name": "rgaa deborah",
	"version": "1.0",
	"description": "mon outil pour auditer une page web selon le rgaa",
	"icons": { "48": "icon-128.png" },
	"permissions": [ "tabs", "activeTab", "scripting" ],
	"browser_action": {
		"default_icon": "icon-128.png",
		"default_title": "rgaa deborah",
		"default_popup": "menu.html"
	},
	"browser_specific_settings": { "gecko": { "id": "rgaa@deborah-powers.fr" }}
}